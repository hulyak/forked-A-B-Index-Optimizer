# Production Environment Configuration
NODE_ENV=production
PORT=3000

# Tiger Data Configuration (set these in your deployment)
TIGER_SERVICE_NAME=your-production-service
TIGER_SERVICE_ID=your-service-id
TIGER_DATABASE_URL=your-tiger-connection-string
TIGER_CLI_PATH=/usr/local/bin/tiger

# Security
ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com
API_KEY=your-secure-api-key-here

# Performance
MAX_CONCURRENT_OPTIMIZATIONS=5
OPTIMIZATION_TIMEOUT=300000
REQUEST_TIMEOUT=30000

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Health Check
HEALTH_CHECK_INTERVAL=30000

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100