{
  "name": "forked-ab-index-optimizer",
  "version": "1.0.0",
  "description": "A/B test database indexes using Agentic Postgres forks and MCP agents",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "server": "node server/index.js",
    "client:dev": "cd client && npm run dev",
    "client:install": "cd client && npm install",
    "client:build": "cd client && npm run build",
    "build": "cd client && npm run build",
    "start": "NODE_ENV=production node server/index.js",
    "install-all": "npm install && cd client && npm install && cd ../mcp && npm install",
    "test": "echo 'Tests not implemented yet'",
    "lint": "echo 'Linting not implemented yet'",
    "clean": "rm -rf client/dist && rm -rf node_modules/.cache"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "pg": "^8.11.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "agentic-postgres",
    "tiger-data",
    "mcp",
    "database",
    "indexing",
    "hackathon"
  ],
  "author": "Your Name",
  "license": "MIT"
}
